          <!--/stories-->
          <div class="container text-center m-5 p-5">

            <div class="container text-center col-6 mt-5" id="myform" style="border: 200px;">
              <h2>Sign Up</h2>
              <br>
              <h6>Create your Library account.</h6>
                  
              <form method="GET" action="" id="form" class="form"  target="_blank" onsubmit="validate()">
              <div class="flex-container" >
                <div class="flex-child form-control">
                  <label for="firstname">First Name</label>
                    <input type="text" placeholder="eg:John" id="firstname"/>
                      <i class="fas fa-check-circle"></i>
                      <i class="fas fa-exclamation-circle"></i>
                      <small>Error message</small>
                </div>
                <div class="flex-child form-control">
                  <label for="lastname">Last Name</label>
                  <input type="text" placeholder="eh:Doe" id="lastname"/>
                  <i class="fas fa-check-circle"></i>
                  <i class="fas fa-exclamation-circle"></i>
                  <small>Error message</small>
                </div>
                </div>
                <div class="mb-3 form-control">
                  <label for="Phone Number">Phone Number</label>
                  <input type="tel" placeholder="Enter Phone number as XXX-XXX-XXX or XXX.XXX.XXX or XXX XXX XXX" id="phone" pattern="/(^\d{3}-\d{3}-\d{4})|(^[\d{3}.\d{3}.\d{4})|(^\d{3}\s\d{3}\s\d{4})/"/>
                  <i class="fas fa-check-circle"></i>
                  <i class="fas fa-exclamation-circle"></i>
                  <small>Error message</small>
                </div>
                <div class="mb-3 form-control">
                  <label for="Email">Email</label>
                  <input type="email" placeholder="Enter your Email Id" id="email"/>
                  <i class="fas fa-check-circle"></i>
                  <i class="fas fa-exclamation-circle"></i>
                  <small>Error message</small>
                </div>
                <div class="mb-3 form-control">
                  <label for="Password">Password</label>
                  <input type="password" placeholder="Enter password" id="password"/>
                  <div class="pw-display-toggle-btn">
                  <i class="fas fa-eye" id ="myEye" ></i>
                  <i class="fas fa-eye-slash" id ="myEye" ></i>
                </div>
                <div class="pw-strength">
                  <span>Weak</span>
                  <span></span>
                </div>
                  <small>Error message</small>
                </div>
                <div class="mb-3 form-control">
                  <label for="Password Confirmation">Confirm Password</label>
                  <input type="password" placeholder="Re-enter password" id="password2"/>
                  <div class="pw-display-toggle-btn">

                </div>
                  <small>Error message</small>
                </div>
                <button type="submit" name="submitButton">Sign Up</button>
            </form>
        
                <p class="para-2">
                    Already have an account? <a href="./login">Login</a>
                </p>
    </div>
          
